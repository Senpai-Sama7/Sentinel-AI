# Secret key for JWT authentication
JWT_SECRET=your_jwt_secret_here
# API key for OpenAI embeddings
OPENAI_API_KEY=your_openai_api_key_here
# Redis connection URL for caching
REDIS_URL=redis://localhost:6379/0
# Chroma host for vector storage
CHROMA_HOST=localhost
# Chroma port
CHROMA_PORT=8000
# Allowed CORS origins
APP_CORS_ORIGINS=["*"]
# Log level for application
LOG_LEVEL=INFO
# Weaviate endpoint for L2 storage
WEAVIATE_URL=http://localhost:8080
# Local git repository path for ingestion
GIT_REPO_PATH=/path/to/repo
# In-memory cache size
L0_CACHE_SIZE=128
# Go proxy gRPC address
GO_PROXY_GRPC_ADDR=localhost:8081
# Path for Chroma local DB
CHROMA_DB_PATH=/tmp/chroma

